<script language="javascript" src="jquery.min.js"></script>
<script language="javascript" src="js/bootstrap.min.js"></script>
<script language="javascript" src="js/brands.min.js"></script>
<script language="javascript" src="js/fontawesome.min.js"></script>
<script language="javascript" src="customized-svg.js"></script>
<link rel="stylesheet" href="css/bootstrap.min.css" />
<link rel="stylesheet" href="bootstrap-social.css" />

<% providers.forEach(provider => { %>
    <a class="btn btn-block btn-social btn-<%= provider.style %>" href="/auth/<%= provider.directory %>"><span class="fab fa-<%= provider.style %>"></span> Sign In with <%= provider.description %></a>
<% }); %>
